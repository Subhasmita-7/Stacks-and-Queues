Example 4:
Input: s = "([])"
Output: true

Example 5:
Input: s = "([)]"
Output: false



class Solution {
    public boolean isValid(String s) {
        Stack<Character> st = new Stack<>();
        Map<Character,Character> mp = new HashMap<>();
        mp.put(')','(');
        mp.put(']','[');
        mp.put('}','{');
        for(char c : s.toCharArray()){
            if(c == '(' || c == '[' || c == '{'){
                st.push(c);
            }
            else if(c == ')' || c == ']' || c == '}'){
                if(st.isEmpty()){
                    return false;
                }
                char check = st.pop();
                if(check != mp.get(c)){
                    return false;
                }
            }
        }
        return st.isEmpty();
    }
}
